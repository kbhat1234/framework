package com.java.practice;

public class OperatorsTest {

	public static void main(String[] args) {
		Operators op = new Operators();
		op.unaryOperation();
		op.unaryAddition();
		op.unaryOperators();
		op.airthmeticOperation();
		op.expr();
		op.leftshiftOperator();
		op.rightshiftOperator();
		op.rightOperator();
		op.logicalAndBitwiseAnd();
		op.logicalOrBitwiseOr();
		op.ternaryOperator();
		op.assignmentOperator();
	}
};