package com.java.practice;

public class Operators {
	
	void unaryOperation() {
		System.out.println("Unary operation");
		int x=10;
		System.out.println(x++);
		System.out.println(x);
		System.out.println(++x);
		System.out.println(x);
		System.out.println(x--);
		System.out.println(x);
		System.out.println(--x);
		System.out.println(x);
	}
}
