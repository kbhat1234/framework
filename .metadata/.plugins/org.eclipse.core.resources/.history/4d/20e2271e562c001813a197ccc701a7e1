package com.java.practice;

public class Operators {
	
	void unaryOperation() {
		System.out.println("Unary operation");
		int x=10;
		System.out.println(x++);
		System.out.println(x);
		System.out.println(++x);
		System.out.println(x);
		System.out.println(x--);
		System.out.println(x);
		System.out.println(--x);
		System.out.println(x);
	}
	void unaryAddition() {
		int x=10;
		int y=20;
		System.out.println("Unary addition");
		int z = x++ + y++;
		System.out.println(x);
		System.out.println(y);
		System.out.println(z);
		
		int m = ++x + ++y;
		System.out.println(x);
		System.out.println(y);
		System.out.println(m);
	}
};
